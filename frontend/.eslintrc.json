{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "extends": "next",
  "rules": {
    "no-console": "warn",
    "no-empty": "warn",
    "no-cond-assign": ["error", "always"],
    "eqeqeq": ["error", "always"],
    "no-eval": "error",
    "no-multi-spaces": "error",
    "no-delete-var": "error",
    "no-unused-vars": "warn",
    "prefer-const": "warn"
  }
}
