{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "next",
  "rules": {
    "no-console": "warn",
    "no-empty": "warn",
    "no-cond-assign": ["error", "always"],
    "eqeqeq": ["error", "always"],
    "no-eval": "error",
    "no-multi-spaces": "error",
    "no-delete-var": "error",
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "react/function-component-definition": [2, {
      "namedComponents": "arrow-function"
    }]
  }
}
